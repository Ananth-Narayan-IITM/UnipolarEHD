sample
{
	type    sets;
	libs    (sampling);
	enabled true;

	

	setFormat       raw;
	interpolationScheme cell;
	writeControl   timeStep;
    	writeInterval  100;

	purgeWrite      30;

	fields (chargeDensity);
sets
{
    Y1
    {
        type        uniform;
        axis        distance;

        start       (0.302 0 0);
        end         (0.302 1 0);
        nPoints     300;
    }
}
}
