FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      probeFunctions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //




probes
{
    name    probes;
    type    probes;

    libs    (sampling);

    writeControl   timeStep;
    writeInterval  1;

    probeLocations
    (
        (0.4 0.05 0)
        (0.55 0.55 0)
        (0.55 0.02 0)
    );

    fields
    (
        chargeDensity p potentialDifference U
    );
}

